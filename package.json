{
  "name": "Electolator",
  "version": "0.5.0",
  "description": "Very basic calculator made with React and Electron. Do not attempt to do your tax return with it.",
  "author": "Matthew Balaam",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "testDev": "electron .",
    "testProd": "electron . --noDevServer",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "packageoverwrite": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds --overwrite"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.1",
    "css-loader": "^0.28.1",
    "electron": "^1.7",
    "electron-packager": "^8.7.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "postcss-cli": "*",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "style-loader": "^0.17.0",
    "webpack": "^3",
    "webpack-dev-server": "^2.4.5"
  }
}
